  "ZWaveDevice": {
    "@xmlns": "http://www.pepper1.net/zwavedb/xml-schemata/z-wave",
    "@schemaVersion": "2",
    "descriptorVersion": "1",
    "deviceData": {
      "manufacturerId": {
        "@value": "0000"
      },
      "productType": {
        "@value": "0100"
      },
      "productId": {
        "@value": "0100"
      },
      "libType": {
        "@value": "06"
      },
      "protoVersion": {
        "@value": "03"
      },
      "protoSubVersion": {
        "@value": "43"
      },
      "appVersion": {
        "@value": "01"
      },
      "appSubVersion": {
        "@value": "52"
      },
      "basicClass": {
        "@value": "04"
      },
      "genericClass": {
        "@value": "10"
      },
      "specificClass": {
        "@value": "01"
      },
      "optional": {
        "@value": "true"
      },
      "listening": {
        "@value": "true"
      },
      "routing": {
        "@value": "true"
      },
      "beamSensor": "0",
      "rfFrequency": "EU"
    },
    "deviceDescription": {
      "description": {
        "lang": [
          {
            "@xml:lang": "de",
            "#text": "Ein Z-Wave gesteuertes Kabel-Schalter welcher entweder normal genutzt oder via Funkkommando geschalten werden kann."
          },
          {
            "@xml:lang": "en",
            "#text": "A Z-wave cord switch which is small enough to be compared to a regular cord switch. It can be used both manually\r\nlike any ordinary cord switch and remotely by Z -Wave ."
          }
        ]
      },
      "wakeupNote": {
        "lang": [
          {
            "@xml:lang": "de"
          },
          {
            "@xml:lang": "en"
          }
        ]
      },
      "inclusionNote": {
        "lang": [
          {
            "@xml:lang": "de",
            "#text": "Ein kurzer Druck auf den Knopf auf der Rückseite startet den Inklusions bzw. Exklusionsmodus"
          },
          {
            "@xml:lang": "en",
            "#text": "Short click on the Button on the Backside starts the Inclusion/Exclusion-Mode"
          }
        ]
      },
      "productName": "Cord Switch",
      "brandName": "Swiid",
      "productCode": "ZCS1",
      "productVersion": "1.82"
    },
    "commandClasses": {
      "commandClass": [
        {
          "@id": "0020",
          "@inNIF": "false"
        },
        {
          "@id": "0025"
        },
        {
          "@id": "0027"
        },
        {
          "@id": "0072"
        },
        {
          "@id": "0085",
          "@version": "2"
        },
        {
          "@id": "0086"
        }
      ]
    },
    "assocGroups": {
      "assocGroup": [
        {
          "@number": "1",
          "@maxNodes": "5",
          "description": {
            "lang": [
              {
                "@xml:lang": "de",
                "#text": "Gruppe 1\r\nReagiert auf langes Drücken des Schalters."
              },
              {
                "@xml:lang": "en",
                "#text": "Group 1\r\nResponds to a long press."
              }
            ]
          }
        },
        {
          "@number": "2",
          "@maxNodes": "5",
          "description": {
            "lang": [
              {
                "@xml:lang": "de",
                "#text": "Gruppe 1\r\nReagiert auf jeden einzelnen Druck."
              },
              {
                "@xml:lang": "en",
                "#text": "Group 2\r\nResponds on every single-click."
              }
            ]
          }
        }
      ]
    },
    "configParams": {
      "configParam": {
        "@number": "1",
        "@type": "constant",
        "@size": "1",
        "@default": "00",
        "name": {
          "lang": [
            {
              "@xml:lang": "de",
              "#text": "Schaltmodus"
            },
            {
              "@xml:lang": "en",
              "#text": "Switchmodus"
            }
          ]
        },
        "description": {
          "lang": [
            {
              "@xml:lang": "de",
              "#text": "Bestimmt die Verwendung des Schalters\r\nDer Wert 255 bestimmt damti den \"normal\"-Modus (an und aus)."
            },
            {
              "@xml:lang": "en",
              "#text": "Select the Switch-mode\r\nThe Value 255 is the normal-mode(switch on and off)"
            }
          ]
        },
        "value": [
          {
            "@from": "00",
            "@to": "00",
            "description": {
              "lang": [
                {
                  "@xml:lang": "de",
                  "#text": "Deaktiviert"
                },
                {
                  "@xml:lang": "en",
                  "#text": "Disabled"
                }
              ]
            }
          },
          {
            "@from": "01",
            "@to": "01",
            "description": {
              "lang": [
                {
                  "@xml:lang": "de",
                  "#text": "Schaltet an"
                },
                {
                  "@xml:lang": "en",
                  "#text": "Switch off only"
                }
              ]
            }
          },
          {
            "@from": "02",
            "@to": "02",
            "description": {
              "lang": [
                {
                  "@xml:lang": "de",
                  "#text": "Schaltet aus"
                },
                {
                  "@xml:lang": "en",
                  "#text": "Switch off only"
                }
              ]
            }
          }
        ]
      }
    }
