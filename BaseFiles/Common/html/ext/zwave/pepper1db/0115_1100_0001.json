              "@from": "00",
              "@to": "00",
              "description": {
                "lang": [
                  {
                    "@xml:lang": "de",
                    "#text": "Ausschalten"
                  },
                  {
                    "@xml:lang": "en",
                    "#text": "Switch off"
                  },
                  {
                    "@xml:lang": "ru",
                    "#text": "Выключить нарузку"
