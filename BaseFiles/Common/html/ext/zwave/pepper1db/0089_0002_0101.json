    "configParams": {
      "configParam": [
        {
          "@number": "0",
          "@type": "constant",
          "@size": "1",
          "@default": "00",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Last Status"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en"
            }
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "To enable the module to resume the last status\r\nbefore the AC power goes down."
                }
              }
            },
            {
              "@from": "01",
              "@to": "01",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "To enable the module to always turn the lights\r\nON when the AC power goes up."
                }
              }
            },
            {
              "@from": "02",
              "@to": "02",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "To enable the module to always turn the lights\r\nOFF when AC power line goes up."
                }
              }
            }
          ]
        },
        {
          "@number": "1",
          "@type": "constant",
          "@size": "1",
          "@default": "01",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Night Light"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en"
            }
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "When the light is turn ON, LED on the module is\r\nturn ON and vice versa."
                }
              }
            },
            {
              "@from": "01",
              "@to": "01",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "When the light is turn ON, LED on the module is\r\nturn OFF and vice versa."
                }
              }
            }
          ]
        },
        {
          "@number": "2",
          "@type": "constant",
          "@size": "1",
          "@default": "01",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Enable LED"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en"
            }
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Disable LED."
                }
              }
            },
            {
              "@from": "01",
              "@to": "01",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Enable LED to operate according to night light\r\nparameter."
                }
              }
            }
          ]
        },
        {
          "@number": "3",
          "@type": "constant",
          "@size": "1",
          "@default": "ff",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Enable All ON/All OFF"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en"
            }
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Disable both All ON and All OFF command."
                }
              }
            },
            {
              "@from": "01",
              "@to": "01",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Enable only All OFF command."
                }
              }
            },
            {
              "@from": "02",
              "@to": "02",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Enable only All ON command."
                }
              }
            },
            {
              "@from": "ff",
              "@to": "ff",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Enable both All ON and All OFF command."
                }
              }
            }
          ]
        },
        {
          "@number": "4",
          "@type": "constant",
          "@size": "1",
          "@default": "00",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Button Protection"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en"
            }
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "User can use button for ON/OFF manual control."
                }
              }
            },
            {
              "@from": "01",
              "@to": "01",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "This setting value is used to protect a module\r\nagainst unintentional control. To operate a\r\nmanual control requires pressing button 3 times\r\nwithin 1.5 seconds, the light will toggle to the\r\nnext stage. The light will turn off instantly if it is\r\nbeing on before setting."
                }
              }
            },
            {
              "@from": "02",
              "@to": "02",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Disable button from manual control. The light\r\nwill turn off instantly if it is being on before\r\nsetting."
                }
              }
            }
          ]
        },
        {
          "@number": "5",
          "@type": "range",
          "@size": "1",
          "@default": "07",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Fade In Slope"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en"
            }
          },
          "value": {
            "@from": "00",
            "@to": "0e",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "These valid values control a ramp up time\r\nperiod. With default value (7), the lights will\r\nturn on from zero to the last dimming level by 7\r\nseconds. Usually this command designed for\r\nprolonging a filament and making the room to\r\nbe more attraction."
              }
            }
          }
        },
        {
          "@number": "6",
          "@type": "range",
          "@size": "1",
          "@default": "07",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Fade Out Slope"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en"
            }
          },
          "value": {
            "@from": "00",
            "@to": "0e",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "Fade in Slope and Fade Out Slope is the same\r\nconcept in which Fade Out Slope command\r\ncontrols a turn off time period, so Fade Out\r\nSlope will ramp an intensity of the light down to\r\nzero within 7 seconds if the valid value sets to\r\nthe default."
              }
            }
          }
        },
        {
          "@number": "7",
          "@type": "range",
          "@size": "1",
          "@default": "07",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Dim Slope"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en"
            }
          },
          "value": {
            "@from": "00",
            "@to": "0e",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "Dim Slope command can control a dim step by\r\nshrinking or extending dimming time period. If\r\nthe value sets to the default, the dimming step\r\nduration will be 7 seconds from minimum to\r\nmaximum level or vice versa."
              }
            }
          }
        },
        {
          "@number": "8",
          "@type": "range",
          "@size": "1",
          "@default": "14",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Minimum Dim Level"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en"
            }
          },
          "value": {
            "@from": "01",
            "@to": "32",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "This is a minimum threshold value to avoid the\r\nconfusion between low dim value stage and\r\nturn-off stage especially with the low wattage\r\nlamps. The light bulbs will illuminate a little\r\nportion of intensity if the value sets to the\r\ndefault (20)."
              }
            }
          }
        },
        {
          "@number": "9",
          "@type": "range",
          "@size": "1",
          "@default": "63",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Maximum Dim Level"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en"
            }
          },
          "value": {
            "@from": "33",
            "@to": "63",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "Maximum dim level is a mirror of minimum dim\r\nlevel that means it is used to defining the\r\nheadroom of the light level which does not\r\nexceed default value (99). The Maximum dim\r\nlevel will influence the value of maximum on\r\nlevel in multilevel switch command class."
              }
            }
          }
        },
        {
          "@number": "10",
          "@type": "constant",
          "@size": "1",
          "@default": "01",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Ignore Start Level"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en"
            }
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "This value indicated to use the start level\r\nspecified in the Multilevel Switch Start Level\r\nChange Command."
                }
              }
            },
            {
              "@from": "01",
              "@to": "01",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "This indicates to start dimming from the latest\r\nlight level."
                }
              }
            }
          ]
        },
        {
          "@number": "11",
          "@type": "constant",
          "@size": "1",
          "@default": "00",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Jump to Dim"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en"
            }
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Disable Jump to dim level. The master remote\r\ncontrol cannot dim the lights from the off stage"
                }
              }
            },
            {
              "@from": "01",
              "@to": "01",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Enable Jump to dim level which means user can\r\njump to dim stage from the off stage without\r\nturning on the light."
                }
              }
            }
          ]
        },
        {
          "@number": "12",
          "@type": "constant",
          "@size": "1",
          "@default": "02",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Dim Direction"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en"
            }
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "The first time user dims the light, it will go in\r\nthe dim up direction to the maximum dim level\r\nand follow by dim down direction respectively."
                }
              }
            },
            {
              "@from": "01",
              "@to": "01",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "The first time user dims the light, it will go in\r\nthe dim down direction to the minimum dim\r\nlevel and follow by dim up direction respectively."
                }
              }
            },
            {
              "@from": "02",
              "@to": "02",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "The dim direction will be a toggle mode. The\r\nfirst time user dims the light it will go in\r\nopposite direction of previous dim direction."
                }
              }
            }
          ]
        },
        {
          "@number": "255",
          "@type": "constant",
          "@size": "1",
          "@default": "01",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "FACTORY DEFAULT"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Valid Values = Ignore configuration value\r\n\r\nThis command will reset the whole above command values to the\r\ndefault factory values"
            }
          }
        }
      ]
    },
