          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Enable Shade Control Group 3"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "The switch can control shade control devices if this parameter is set to 1."
            }
          }
        },
        {
          "@number": "19",
          "@type": "constant",
          "@size": "1",
          "@default": "02",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "LED Transmission Indication"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "The WD500Z-1 will flicker its LED when it is transmitting to any of its groups.\r\nParameter set to 0 = not flicker\r\nParameter set to 1 = flicker the entire time of transmitting\r\nParameter set to 2 = flicker for only 1 second"
