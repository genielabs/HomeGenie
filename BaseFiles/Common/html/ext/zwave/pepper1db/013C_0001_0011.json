        }
      },
      "inclusionNote": {
        "lang": {
          "@xml:lang": "en",
          "#text": "Pressing On/Off button three times\r\nwithin 2 seconds will enter inclusion mode."
        }
      },
      "productName": "Smart Energy Plug In Switch",
