              }
            },
            {
              "@from": "01",
              "@to": "01",
              "description": {
                "lang": [
                  {
                    "@xml:lang": "de",
                    "#text": "Normaler Modus"
                  },
                  {
                    "@xml:lang": "en",
                    "#text": "Normal Mode"
                  }
                ]
              }
            }
          ]
        },
