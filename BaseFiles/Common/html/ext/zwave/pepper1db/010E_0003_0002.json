    "configParams": {
      "configParam": [
        {
          "@number": "1",
          "@type": "constant",
          "@size": "1",
          "@default": "00",
          "name": {
            "lang": [
              {
                "@xml:lang": "de",
                "#text": "Drehrichtung"
              },
              {
                "@xml:lang": "en",
                "#text": "Turning Direction"
              }
            ]
          },
          "description": {
            "lang": [
              {
                "@xml:lang": "de",
                "#text": "Parameter definiert in welche Richtung der Motor das Schloss in Relation zum Schließbolzen dreht"
              },
              {
                "@xml:lang": "en",
                "#text": "parameter defines which direction the motor turns the lock in relation to the latch"
              }
            ]
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": [
                  {
                    "@xml:lang": "de",
                    "#text": "in Uhrzeigerrichtung beim Schließen"
                  },
                  {
                    "@xml:lang": "en",
                    "#text": "Clockwise when latching"
                  }
                ]
              }
            },
            {
              "@from": "01",
              "@to": "01",
              "description": {
                "lang": [
                  {
                    "@xml:lang": "de",
                    "#text": "gegen Uhrzeigerrichtung beim Schließen"
                  },
                  {
                    "@xml:lang": "en",
                    "#text": "Counterclockwise when latching"
                  }
                ]
              }
            }
          ]
        },
        {
          "@number": "2",
          "@type": "constant",
          "@size": "1",
          "@default": "03",
          "name": {
            "lang": [
              {
                "@xml:lang": "de",
                "#text": "Drehgeschwindigkeit (Drehmoment)"
              },
              {
                "@xml:lang": "en",
                "#text": "Turning Speed (Torque)"
              }
            ]
          },
          "description": {
            "lang": [
              {
                "@xml:lang": "de",
                "#text": "Parameter definiert, ob Motor langsam (mehr Drehmoment) oder schnell (weniger Drehmoment) dreht"
              },
              {
                "@xml:lang": "en",
                "#text": "parameter defines whether the motor goes slower (more torque) or faster (less torque) when turning the lock"
              }
            ]
          },
          "value": [
            {
              "@from": "01",
              "@to": "01",
              "description": {
                "lang": [
                  {
                    "@xml:lang": "de",
                    "#text": "niedrigste Geschwindigkeit, höchstes Drehmoment"
                  },
                  {
                    "@xml:lang": "en",
                    "#text": "Slowest Speed, Strongest Torque"
                  }
                ]
              }
            },
            {
              "@from": "02",
              "@to": "02",
              "description": {
                "lang": [
                  {
                    "@xml:lang": "de",
                    "#text": "niedrige Geschwindigkeit, hoches Drehmoment"
                  },
                  {
                    "@xml:lang": "en",
                    "#text": "Slow Speed, Stronger Torque"
                  }
                ]
              }
            },
            {
              "@from": "03",
              "@to": "03",
              "description": {
                "lang": [
                  {
                    "@xml:lang": "de",
                    "#text": "normale Geschwindigkeit, normales Drehmoment"
                  },
                  {
                    "@xml:lang": "en",
                    "#text": "Normal Speed,\tNormal Torque"
                  }
                ]
              }
            },
            {
              "@from": "04",
              "@to": "04",
              "description": {
                "lang": [
                  {
                    "@xml:lang": "de",
                    "#text": "hohe Geschwindigkeit, geringes Drehmoment"
                  },
                  {
                    "@xml:lang": "en",
                    "#text": "Fast Speed, Weak Torque"
                  }
                ]
              }
            },
            {
              "@from": "05",
              "@to": "05",
              "description": {
                "lang": [
                  {
                    "@xml:lang": "de",
                    "#text": "höchste Geschwindigkeit, geringstes Drehmoment"
                  },
                  {
                    "@xml:lang": "en",
                    "#text": "Fastest Speed, Weakest Torque"
                  }
                ]
              }
            }
          ]
        },
        {
          "@number": "3",
          "@type": "constant",
          "@size": "1",
          "@default": "01",
          "name": {
            "lang": [
              {
                "@xml:lang": "de",
                "#text": "Drehmoment-Einstellung"
              },
              {
                "@xml:lang": "en",
                "#text": "Torque Setup"
              }
            ]
          },
          "description": {
            "lang": [
              {
                "@xml:lang": "de",
                "#text": "Parameter definiert ob Motor mit viel oder wenig Drehmoment dreht beim Öffnen/Schließen (weniger Drehmoment spart Energie)"
              },
              {
                "@xml:lang": "en",
                "#text": "parameter defines if more or less torque is used at latch/unlatch operations (less torque safes power)"
              }
            ]
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": [
                  {
                    "@xml:lang": "de",
                    "#text": "geringes Drehmoment, Energiespar-Modus"
                  },
                  {
                    "@xml:lang": "en",
                    "#text": "Less torque, Power Saving mode\t"
                  }
