                "@xml:lang": "en",
                "#text": "Turning degrees"
              }
            ]
          },
          "description": {
            "lang": [
              {
                "@xml:lang": "de",
                "#text": "Parameter definiert wieviel Grad das Schloss dreht bei Öffnen/Schließen"
              },
              {
                "@xml:lang": "en",
                "#text": "parameter defines how many degrees the lock turns at latch/unlatch operations"
              }
            ]
          },
          "value": {
            "@from": "01",
            "@to": "64",
            "@unit": "*10°",
            "description": {
              "lang": [
                {
                  "@xml:lang": "de",
                  "#text": "Faktor 10;\r\n1 = 10°;\r\n72 = 720° = 2 Umdrehungen"
                },
                {
                  "@xml:lang": "en",
                  "#text": "factor 10;\r\n1 = 10°;\r\n72 = 720° = 2 turns"
                }
              ]
            }
          }
        },
        {
          "@number": "5",
          "@type": "range",
          "@size": "1",
          "@default": "00",
          "name": {
            "lang": [
              {
                "@xml:lang": "de",
                "#text": "Auto Schließ-Zeit Verzögerung"
              },
              {
                "@xml:lang": "en",
                "#text": "Auto Latch Time Delay"
              }
            ]
          },
          "description": {
            "lang": [
              {
                "@xml:lang": "de",
                "#text": "Parameter definiert nach wievielen Sekunden nach dem Öffnen das Schloss automatisch wieder schließt"
              },
              {
                "@xml:lang": "en",
                "#text": "parameter defines amount of seconds after opening the door automatically locks again"
              }
            ]
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": [
                  {
                    "@xml:lang": "de",
                    "#text": "Inaktiv"
                  },
                  {
                    "@xml:lang": "en",
                    "#text": "Disabled"
                  }
                ]
              }
            },
            {
              "@from": "01",
              "@to": "3c",
              "@unit": "s",
              "description": {
                "lang": [
                  {
                    "@xml:lang": "de",
                    "#text": "Verzögerung nach dem Öffnen in Sekunden"
                  },
                  {
                    "@xml:lang": "en",
                    "#text": "delay after opening in seconds"
                  }
                ]
              }
            }
          ]
        },
        {
          "@number": "6",
          "@type": "constant",
          "@size": "1",
          "@default": "01",
          "name": {
            "lang": [
              {
                "@xml:lang": "de",
                "#text": "Klang-Einstellungen"
              },
              {
                "@xml:lang": "en",
                "#text": "Sound setup"
              }
            ]
          },
          "description": {
            "lang": [
              {
                "@xml:lang": "de",
                "#text": "Parameter definiert, ob ein akustisches Signal das Öffnen/Schließen bestätigt"
              },
              {
                "@xml:lang": "en",
                "#text": "parameter defines if an acoustic signal confirms latch/unlatch operations"
              }
            ]
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": [
                  {
                    "@xml:lang": "de",
                    "#text": "akustisches Signal deaktiviert"
                  },
                  {
                    "@xml:lang": "en",
                    "#text": "Latch/unlatch Sound disabled "
                  }
                ]
              }
            },
            {
              "@from": "01",
              "@to": "01",
              "description": {
                "lang": [
                  {
                    "@xml:lang": "de",
                    "#text": "akustisches Signal aktiviert"
                  },
                  {
                    "@xml:lang": "en",
                    "#text": "Latch/unlatch Sound enabled"
                  }
                ]
              }
            }
          ]
        },
        {
          "@number": "7",
          "@type": "constant",
          "@size": "1",
          "@default": "01",
          "name": {
            "lang": [
              {
                "@xml:lang": "de",
                "#text": "Batterie-Typ"
              },
              {
                "@xml:lang": "en",
                "#text": "Battery Type"
              }
            ]
          },
          "description": {
            "lang": [
              {
                "@xml:lang": "de",
                "#text": "Parameter definiert mit welchem Batterie-Typ (Spannung) das Gerät betrieben wird; Wert sollte nicht geändert werden!"
              },
              {
                "@xml:lang": "en",
                "#text": "parameter defines the battery type (voltage) is used to power the device; value shouldn't be changed!"
              }
            ]
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": [
                  {
                    "@xml:lang": "de",
                    "#text": "3.6 Volt Batterie-Typ"
                  },
                  {
                    "@xml:lang": "en",
                    "#text": "3.6 volt battery type"
                  }
                ]
              }
            },
            {
              "@from": "01",
              "@to": "01",
              "description": {
                "lang": [
                  {
                    "@xml:lang": "de",
                    "#text": "3.0 Volt Batterie-Typ"
                  },
                  {
                    "@xml:lang": "en",
                    "#text": "3.0 volt battery type"
                  }
                ]
              }
            }
          ]
        },
        {
          "@number": "8",
          "@type": "range",
          "@size": "1",
          "@default": "1e",
          "name": {
            "lang": [
              {
                "@xml:lang": "de",
                "#text": "Batterie Alarm Wert"
              },
              {
                "@xml:lang": "en",
                "#text": "Battery Alarm Value Threshold"
              }
            ]
          },
          "description": {
            "lang": [
              {
                "@xml:lang": "de",
                "#text": "Parameter definiert einen Batterie-Status Prozentwert, der beim Unterschreiten zu einem akustischen Signal nach dem Öffnen/Schließen führt"
              },
              {
                "@xml:lang": "en",
                "#text": "parameter defines the percent battery status threshold underrun which leads to an acoustic signal after latch/unlatch operations"
              }
            ]
          },
          "value": {
            "@from": "00",
            "@to": "64",
            "@unit": "%",
            "description": {
              "lang": [
                {
                  "@xml:lang": "de",
                  "#text": "0-100%"
                },
                {
                  "@xml:lang": "en",
                  "#text": "0-100%"
                }
              ]
            }
          }
        }
      ]
