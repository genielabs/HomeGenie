            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "IN1 and IN2 Broadcast inactive"
                }
              }
            },
            {
              "@from": "01",
              "@to": "01",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "IN1 broadcast mode active, Sensor 2 broadcast mode inactive"
