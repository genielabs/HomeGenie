    "deviceDescription": {
      "description": {
        "lang": {
          "@xml:lang": "en",
          "#text": "Electronic thermostat with wireless communication via Z-Wave. The thermostat can control a floor sensor,a room sensor, a rooms sensor with a max limitor in the floor, 2 external sensors and also be used as a power regulator. In all of these modes the thermostat can withstand a load of 16A/3600W."
        }
      },
      "wakeupNote": {
        "lang": {
          "@xml:lang": "en"
        }
      },
      "inclusionNote": {
        "lang": {
          "@xml:lang": "en",
          "#text": "Press button 3 till you see On. Then confirm with button 2. Look for the inclusion\\exclusion symbol for confirmation. When added in gateway, Inc or Ecl will appear on the screen. If pairing fails, Err will appear."
        }
      },
      "productName": "Multireg Z-Wave electronic thermostat with 4 functions",
      "brandName": "Multireg",
      "productLine": "Multireg thermostats",
      "productCode": "MR Z-Wave"
