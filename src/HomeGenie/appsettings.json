{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "IncludeScopes": true,
        "SingleLine": true,
        "TimestampFormat": "yyyy-MM-ddTHH:mm:ss.fffffffzzz "
      }
    },
    "File": {
      "TimestampFormat": "yyyy-MM-ddTHH:mm:ss.fffffffzzz "
    }
  },
  "AllowedHosts": "*"
}
